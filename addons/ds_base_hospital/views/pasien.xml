<?xml version='1.0' encoding='utf-8'?>
<odoo>
        <data>
          <!-- ds.pasien tree view -->
            <record id="ds_pasien_view_tree" model="ir.ui.view">
              <field name="name">ds.pasien.view.tree</field>
              <field name="model">ds.pasien</field>
              <field name="arch" type="xml">
            <tree>

            <!-- Add your fields here -->
                  <field name="name"/>
                  <field name="nik"/>
                  <field name="gender"/>
                  <field name="birth_date"/>
                  </tree>
              </field>
            </record>

            <!-- ds.pasien form view -->
            <record id="ds_pasien_view_form" model="ir.ui.view">
              <field name="name">ds.pasien.view.form</field>
              <field name="model">ds.pasien</field>
              <field name="arch" type="xml">
                <form string="">
                  <sheet>
                    <group>
                      <!-- Add your fields here -->
                        <field name="name"/>
                  <field name="nik"/>
                  <field name="gender"/>
                  <field name="birth_date"/>
                  <field name="phone"/>
                  <field name="address"/>
                      
                    </group>
                  </sheet>
                </form>
              </field>
            </record>
            
